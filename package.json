{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e frontend/application.js -o app/assets/javascripts/application.js",
    "serve": "./bin/rails server -b 0.0.0.0 -p 3000",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify frontend/application.js | uglifyjs -c warnings=false -m > app/assets/javascripts/application.js",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "chart.js": "^2.7.2",
    "date-fns": "^1.28.1",
    "vue": "^2.5.13",
    "vue-router": "^2.3.0",
    "vueify": "^9.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^7.2.0",
    "browserify": "^16.2.3",
    "browserify-hmr": "^0.3.7",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "http-server": "^0.10.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.11.1"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}

{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e frontend/application.js -o app/assets/javascripts/application.js",
    "serve": "./bin/rails server -b 0.0.0.0 -p 3000",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify frontend/application.js | uglifyjs -c warnings=false -m > app/assets/javascripts/application.js",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@rails/webpacker": "^5.4.3",
    "axios": "^1.12.0",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "js-yaml": "^4.1.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-template-compiler": "^2.7.16",
    "vueify": "^9.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.26.8",
    "babelify": "^10.0.0",
    "browserify": "^17.0.1",
    "browserify-hmr": "^0.4.1",
    "cross-env": "^7.0.3",
    "envify": "^4.1.0",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.19.3",
    "watchify": "^4.0.0",
    "webpack-dev-server": "^5.2.2"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}

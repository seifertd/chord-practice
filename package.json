{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e frontend/application.js -o app/assets/javascripts/application.js",
    "serve": "./bin/rails server -b 0.0.0.0 -p 3000",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify frontend/application.js | uglifyjs -c warnings=false -m > app/assets/javascripts/application.js",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@rails/webpacker": "5.2.1",
    "axios": "^0.21.2",
    "chart.js": "^2.9.3",
    "date-fns": "^1.30.1",
    "js-yaml": "^3.14.0",
    "vue": "^2.6.11",
    "vue-router": "^2.3.0",
    "vue-template-compiler": "^2.6.11",
    "vueify": "^9.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^7.2.0",
    "browserify": "^16.5.1",
    "browserify-hmr": "^0.4.1",
    "cross-env": "^7.0.3",
    "envify": "^4.1.0",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.17.4",
    "watchify": "^4.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
